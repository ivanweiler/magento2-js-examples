<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="empty" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Example 6</title>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="example6" template="Favicode_UiExamples::example6.phtml">
                <arguments>
                    <argument name="jsLayout" xsi:type="array">
                        <item name="types" xsi:type="array"/>
                        <item name="components" xsi:type="array">
                            <item name="hello" xsi:type="array">
                                <item name="component" xsi:type="string">Favicode_UiExamples/js/view/example6</item>
                                <!--
                                <item name="config" xsi:type="array">
                                    <item name="template" xsi:type="string">Favicode_UiExamples/example6</item>
                                </item>
                                -->
                            </item>
                        </item>
                    </argument>
                </arguments>
            </block>
        </referenceContainer>


        <referenceBlock name="messages" remove="true" />
        <referenceBlock name="authentication-popup" remove="true" />
        <referenceBlock name="pageCache" remove="true" />
        <!--referenceBlock name="customer.section.config" remove="true" /-->
        <!--referenceBlock name="customer.customer.data" remove="true" /-->
        <referenceBlock name="translate" remove="true" />

    </body>
</page>
